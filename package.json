{
  "name": "amazocart",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "node ./node_modules/vite/bin/vite.js",
    "dev:all": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "node ./node_modules/vite/bin/vite.js",
    "dev:backend": "cd ../backend && npm start",
    "build": "node ./node_modules/vite/bin/vite.js build",
    "build:dev": "NODE_ENV=development vite build -m development",
    "preview": "node ./node_modules/vite/bin/vite.js preview"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.66.1",
    "react-router-dom": "^6.22.0",
    "i18next": "^23.7.18",
    "react-i18next": "^14.1.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}